<?xml version="1.0" encoding="UTF-8"?>
<preference-headers xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:unified="http://schemas.android.com/apk/res-auto" >

    <!--
	/*
	** Header definitions are identical to android:* except for preferenceRes which should be a reference to a preference xml file
	** Unlike the native headers these are also used for building the single pane version.
	*/
    -->
    <header
        unified:fragment="sk.henrichg.phoneprofiles.PhoneProfilesPreferencesActivity$GUIPreferencesFragment"
        unified:title="@string/phone_profiles_pref_category_gui"
        unified:preferenceRes="@xml/phone_profiles_preferences_gui" />
    <header
        unified:fragment="sk.henrichg.phoneprofiles.PhoneProfilesPreferencesActivity$ActivationPreferencesFragment"
        unified:title="@string/phone_profiles_pref_category_activation"
        unified:preferenceRes="@xml/phone_profiles_preferences_activation" />
    <header
        unified:fragment="sk.henrichg.phoneprofiles.PhoneProfilesPreferencesActivity$NotificationsPreferencesFragment"
        unified:title="@string/phone_profiles_pref_category_notifications"
        unified:preferenceRes="@xml/phone_profiles_preferences_notifications" />
    <header
        unified:fragment="sk.henrichg.phoneprofiles.PhoneProfilesPreferencesActivity$StartPreferencesFragment"
        unified:title="@string/phone_profiles_pref_category_start"
        unified:preferenceRes="@xml/phone_profiles_preferences_start" />
    
</preference-headers>